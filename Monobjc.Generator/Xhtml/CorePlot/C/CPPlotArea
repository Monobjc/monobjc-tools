        <location file="/Users/laurent/Projects/GoogleCode/CorePlot/framework/Source/CPPlotArea.h" line="25" bodyfile="/Users/laurent/Projects/GoogleCode/CorePlot/framework/Source/CPPlotArea.h" bodystart="25" bodyend="-1"/>
      </memberdef>
